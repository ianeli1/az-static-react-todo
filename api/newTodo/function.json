{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "blob",
      "type": "blob",
      "direction": "in",
      "path": "todo/todos.json",
      "dataType": "string"
    },
    {
      "name": "blobOut",
      "type": "blob",
      "direction": "out",
      "path": "todo/todos.json",
      "dataType": "string"
    }
  ],
  "scriptFile": "../dist/newTodo/index.js"
}
